{
  "name": "sessionforge",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:clean": "npm run clean && next dev",
    "clean": "powershell -Command \"$id = (Get-NetTCPConnection -LocalPort 3000 -ErrorAction SilentlyContinue).OwningProcess; if($id) { Stop-Process -Id $id -Force }; Remove-Item -Path .next -Recurse -Force -ErrorAction SilentlyContinue; Write-Host 'Cleanup Complete'\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest run"
  },
