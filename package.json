    "@supabase/supabase-js": "^2.39.0",
    "@types/three": "^0.160.0",
    "canvas": "^3.2.1",
    "lucide-react": "^0.300.0",
    "next": "^16.1.6",
    "react": "^18",
    "react-dom": "^18",
    "sonner": "^2.0.7",
    "three": "^0.160.0"
  },
